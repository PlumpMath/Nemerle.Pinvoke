using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Linq;

using Nemerle.Pinvoke.CParser.Macros;

namespace Nemerle.Pinvoke.CParser.ReflectionTree
{

  [StandardNode]
  public variant TopLevelEntry
  {
    | Typedef 
        {
            Aliases : list[TypedefAlias];
        }

    | TypeDeclaration
        {
            Value : ReflectionTree.TypeDeclaration
        }
    | Function
        {
            Name : string;
            ReturnValue : TypeDeclaration;
            Parameters : list[ParameterDeclaration];
        }
  }
}
